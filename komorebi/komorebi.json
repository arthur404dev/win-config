{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/master/schema.json",
  "active_window_border": true,
  "active_window_border_colours": {
    "single": {
      "r": 203,
      "g": 166,
      "b": 247
    },
    "stack": {
      "r": 245,
      "g": 194,
      "b": 231
    },
    "monocle": {
      "r": 243,
      "g": 139,
      "b": 168
    }
  },
  "active_window_border_offset": -5,
  "active_window_border_width": 8,
  "alt_focus_hack": true,
  "app_specific_configuration_path": "$Env:USERPROFILE/applications.yaml",
  "cross_monitor_move_behaviour": "Insert",
  "default_container_padding": 5,
  "default_workspace_padding": 5,
  "float_rules": [
    {
      "id": "yasb.exe",
      "kind": "Exe"
    }
  ],
  "monitors": [
    {
      "name": "Samsung",
      "workspaces": [
        {
          "layout": "UltrawideVerticalStack",
          "name": "I"
        },
        {
          "layout": "VerticalStack",
          "name": "II"
        },
        {
          "layout": "VerticalStack",
          "name": "III"
        },
        {
          "layout": "UltrawideVerticalStack",
          "name": "IV"
        },
        {
          "layout": "BSP",
          "name": "V"
        }
      ]
    },
    {
      "name": "Bluecase",
      "workspaces": [
        {
          "layout": "BSP",
          "name": "I",
          "initial_workspace_rules": [
            {
              "id": "Slack.exe",
              "kind": "Exe"
            },
            {
              "id": "Discord.exe",
              "kind": "Exe"
            }
          ]
        },
        {
          "layout": "VerticalStack",
          "name": "II"
        },
        {
          "layout": "BSP",
          "name": "III"
        }
      ]
    },
    {
      "name": "Dell",
      "workspaces": [
        {
          "layout": "UltrawideVerticalStack",
          "name": "I"
        },
        {
          "layout": "UltrawideVerticalStack",
          "name": "II"
        },
        {
          "layout": "BSP",
          "name": "III"
        }
      ]
    }
  ],
  "resize_delta": 50,
  "window_hiding_behaviour": "Cloak"
}
